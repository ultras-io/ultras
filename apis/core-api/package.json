{
  "name": "@ultras/core-api",
  "version": "1.0.0",
  "description": "The core api for the ultras app",
  "main": "build/index.js",
  "scripts": {
    "build": "rimraf ./build && tsc --rootDir src",
    "dev": "nodemon",
    "prod": "NODE_ENV=production pm2 start build/api/index.js --env production --name ultras-api",
    "test": "echo Error: no test specified && exit 1",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.(ts|js)' --write",
    "prettier-watch": "onchange 'src/**/*.ts' -- prettier --write {{changed}}",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --ext .ts --fix",
    "migrate": "npx sequelize-cli db:migrate",
    "unmigrate": "npx sequelize-cli db:migrate:undo:all",
    "seed-up": "npx sequelize-cli db:seed:all",
    "seed-down": "npx sequelize-cli db:seed:undo:all"
  },
  "author": "@ultras",
  "license": "ISC",
  "devDependencies": {
    "@types/ioredis": "^4.28.7",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/koa": "^2.13.4",
    "@types/koa-ratelimit": "^4.2.4",
    "@types/koa-router": "^7.4.4",
    "@types/koa__cors": "^3.0.3",
    "@types/lodash": "^4.14.173",
    "@types/node": "^16.10.3",
    "@types/sequelize": "^4.28.10",
    "@types/validator": "^13.6.3",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.32.0",
    "axios": "^0.22.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "module-alias": "^2.2.2",
    "nodemon": "^2.0.12",
    "onchange": "^7.1.0",
    "prettier": "2.4.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@koa/cors": "^3.1.0",
    "@ultras/services": "^1.0.0",
    "@ultras/utils": "^1.0.0",
    "dotenv": "^10.0.0",
    "ioredis": "^4.28.3",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.1",
    "koa-body": "^4.2.0",
    "koa-ratelimit": "^5.0.1",
    "koa-respond": "^2.1.0",
    "koa-router": "^10.1.1",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.6.5"
  }
}
