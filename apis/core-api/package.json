{
  "name": "@ultras/core-api",
  "version": "1.0.0",
  "description": "The core api for the ultras app",
  "main": "build/index.js",
  "scripts": {
    "build": "bash ./scripts/build.sh",
    "dev": "nodemon",
    "prod": "NODE_ENV=production pm2 start build/api/index.js --env production --name ultras-api",
    "test": "echo Error: no test specified && exit 1",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.(ts|js)' --write",
    "prettier-watch": "onchange 'src/**/*.ts' -- prettier --write {{changed}}",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --ext .ts --fix",
    "migrate": "npx sequelize-cli db:migrate",
    "unmigrate": "npx sequelize-cli db:migrate:undo:all",
    "seed-up": "npx sequelize-cli db:seed:all",
    "seed-down": "npx sequelize-cli db:seed:undo:all"
  },
  "author": "@ultras",
  "license": "ISC",
  "dependencies": {
    "@koa/cors": "^4.0.0",
    "@ultras/services": "^1.0.0",
    "@ultras/utils": "^1.0.0",
    "@ultras/view-models": "^1.0.0",
    "aws-sdk": "^2.1252.0",
    "dotenv": "^16.0.3",
    "ioredis": "^5.2.4",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.4",
    "koa-body": "^6.0.1",
    "koa-ratelimit": "^5.0.1",
    "koa-respond": "^2.1.0",
    "koa-router": "^12.0.0",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.25.5",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/ioredis": "^5.0.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/koa": "^2.13.5",
    "@types/koa__cors": "^3.3.0",
    "@types/koa-ratelimit": "^5.0.0",
    "@types/koa-router": "^7.4.4",
    "@types/lodash": "^4.14.188",
    "@types/node": "^18.11.9",
    "@types/sequelize": "^4.28.14",
    "@types/uuid": "^8.3.4",
    "@types/validator": "^13.7.10",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "axios": "^1.1.3",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "module-alias": "^2.2.2",
    "nodemon": "^2.0.20",
    "onchange": "^7.1.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
